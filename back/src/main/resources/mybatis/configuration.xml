<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
    <typeAliases>
        <typeAlias alias="userVo" type="com.example.openchat.vo.UserVo"/>
        <typeAlias alias="communityVo" type="com.example.openchat.vo.CommunityVo"/>
        <typeAlias alias="replyVo" type="com.example.openchat.vo.ReplyVo"/>
        <typeAlias alias="noticeVo" type="com.example.openchat.vo.NoticeVo"/>
        <typeAlias alias="mainVo" type="com.example.openchat.vo.MainVo"/>
        <typeAlias alias="userInfoVo" type="com.example.openchat.vo.UserInfoVo"/>
    </typeAliases>
    <mappers>
        <mapper resource="mybatis/mapper/user.xml"/>
        <mapper resource="mybatis/mapper/community.xml"/>
        <mapper resource="mybatis/mapper/reply.xml"/>
        <mapper resource="mybatis/mapper/notice.xml"/>
        <mapper resource="mybatis/mapper/main.xml"/>
        <mapper resource="mybatis/mapper/admin.xml"/>
    </mappers>

</configuration>